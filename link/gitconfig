[core]
	excludesfile = ~/.gitignore
	editor = vim
[credential]
	helper = osxkeychain
[push]
	default = simple
[alias]
	last = log -1 HEAD
	unstage = reset HEAD --
	st = status
	ci = commit
	co = checkout
	br = branch
	amend = commit --amend
	alias = !git config --list | grep alias | sed s/^......//
	aa = add --all
	cm = commit -m
	dc = diff --cached
	di = diff
